PRO load_dalpha_profile,shot,br,rt,time,peri=peri,fiber=fiber
	nd1=6
	nd2=6
	np1=6
	nch=nd1+nd2+np1
	mdsopen,'dnb',shot
	time=mdsvalue('dim_of(\DNB::TOP.MIT_CXRS.DALPHA:PD1_1,0)')
	ntime=n(time)+1
	br=fltarr(nch,ntime)
	peri=strarr(nch)
	fiber=intarr(nch)
	i=0
	FOR j=1,nd1 DO BEGIN
		br[*,i]=mdsvalue('\DNB::TOP.MIT_CXRS.DALPHA:PD1_+'num2str(j,1))
		peri[i]=mdsvalue('\DNB::TOP.MIT_CXRS.DALPHA:PD1_'+num2str(j,1)+':PERI')
		fiber[i]=mdsvalue('\DNB::TOP.MIT_CXRS.DALPHA:PD1_'+num2str(j,i)+':PERI_FIBER')
		i+=1
        ENDFOR
	FOR j=1,nd2 DO BEGIN
		br[*,i]=mdsvalue('\DNB::TOP.MIT_CXRS.DALPHA:PD2_+'num2str(j,1))
		peri[i]=mdsvalue('\DNB::TOP.MIT_CXRS.DALPHA:PD2_'+num2str(j,1)+':PERI')
		fiber[i]=mdsvalue('\DNB::TOP.MIT_CXRS.DALPHA:PD2_'+num2str(j,i)+':PERI_FIBER')
		i+=1
        ENDFOR
	FOR j=1, DO BEGIN
		br[*,i]=mdsvalue('\DNB::TOP.MIT_CXRS.DALPHA:PMT1_+'num2str(j,1))
		peri[i]=mdsvalue('\DNB::TOP.MIT_CXRS.DALPHA:PMT1_'+num2str(j,1)+':PERI')
		fiber[i]=mdsvalue('\DNB::TOP.MIT_CXRS.DALPHA:PMT1_'+num2str(j,i)+':PERI_FIBER')
		i+=1
        ENDFOR

END
